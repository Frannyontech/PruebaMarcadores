<p id="notice"><%= notice %></p>

<h1 class="text-center">Bookmarks</h1>

<div class="container">
  <div class="row justify-content-center">
    <div class="col-8">
      <%= render 'form', bookmark: @bookmark %>
    </div>
  </div>
</div>

<table class="table table-bordered" id="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Type</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <%= render @bookmarks %>
      </tr>
  </tbody>
</table>

<br>


